		"next": "14.0.4",
		"react": "^18",
		"react-dom": "^18",
		"socket.io": "^4.7.2",
		"socket.io-client": "^4.7.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@emotion/react": "^11.11.3",
		"@types/cors": "^2.8.17",
