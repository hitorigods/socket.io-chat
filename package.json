{
	"name": "socket.io-next",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prisma:studio": "dotenv -e .env.local -- prisma studio",
		"prisma:push": "dotenv -e .env.local -- prisma db push dev",
		"prisma:migrate": "dotenv -e .env.local -- prisma migrate dev --name init",
		"prisma:pull": "dotenv -e .env.local -- prisma db pull dev",
		"prisma:reset": "dotenv -e .env.local -- prisma migrate reset",
		"prisma:generate": "dotenv -e .env.local -- prisma generate",
		"prisma:seed": "dotenv -e .env.local -- prisma db seed"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@fontsource-variable/noto-sans-jp": "^5.0.17",
		"@prisma/client": "5.7.1",
		"@supabase/supabase-js": "^2.39.1",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"jotai": "^2.6.1",
		"next": "14.0.4",
		"react": "^18",
		"react-dom": "^18",
		"socket.io": "^4.7.2",
		"socket.io-client": "^4.7.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@emotion/react": "^11.11.3",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/node": "^20.10.6",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"autoprefixer": "^10.0.1",
		"dotenv": "^16.3.1",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8",
		"eslint-config-next": "14.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-tailwindcss": "^3.13.1",
		"jotai-devtools": "^0.7.1",
		"postcss": "^8",
		"prettier": "^3.1.1",
		"prisma": "^5.7.1",
		"tailwindcss": "^3.3.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	}
}
